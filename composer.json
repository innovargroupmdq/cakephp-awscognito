{
    "name": "evil-corp/cakephp-awscognito",
    "description": "AwsCognito plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "3.5.*",
        "aws/aws-sdk-php": "^3.54",
        "lcobucci/jwt": "^3.2",
        "evil-corp/cakephp-awsapigateway": "dev-master",
        "evil-corp/cakephp-awss3upload": "dev-master",
        "muffin/footprint": "^1.2",
        "friendsofcake/search": "^3.1",
        "ralouphie/mimey": "^2.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@bitbucket.org:evil-corp/cakephp-awsapigateway.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:evil-corp/cakephp-awss3upload.git"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "EvilCorp\\AwsCognito\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EvilCorp\\AwsCognito\\Test\\": "tests",
            "EvilCorp\\AwsCognito\\Test\\Fixture\\": "tests"
        }
    }
}
